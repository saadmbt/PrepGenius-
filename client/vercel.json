{
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/auth(?:/.*)?$",
      "dest": "/index.html"
    },
    {
      "src": "^/professor(?:/.*)?$",
      "dest": "/index.html"
    },
    {
      "src": "^/Student(?:/.*)?$",
      "dest": "/index.html"
    },
    {
      "src": "^/JoinQuiz(?:/.*)?$",
      "dest": "/index.html"
    },
    {
      "src": "^/.*$",
      "dest": "/index.html"
    }
  ]
}
